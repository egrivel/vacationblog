{
  "extends": [
    "google",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "arrow-parens": [2, "always"],
    "no-unused-vars": 2,
    "no-undef": 2,
    "no-var": 2,
    "prefer-const": [2, {
        "destructuring": "any",
        "ignoreReadBeforeAssign": false
    }]
  }
}
